{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React TypeScript application starter",
  "private": true,
  "author": "Martin Litvaj <kamahl19@gmail.com> (http://litvaj.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kamahl19/react-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Kamahl19/react-starter/issues"
  },
  "engines": {
    "node": ">=14.18",
    "yarn": "1.x"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "build": "vite build",
    "format": "prettier --write . --ignore-unknown",
    "format-check": "prettier --check . --ignore-unknown",
    "extract-translations": "i18next",
    "lint": "eslint \"./**/*.{js,ts,tsx}\" --cache",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --cache"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.3.0",
    "@types/node": "18.6.1",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "@vitejs/plugin-react": "2.0.0",
    "@vitest/ui": "0.19.0",
    "autoprefixer": "10.4.7",
    "confusing-browser-globals": "1.0.11",
    "eslint": "8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.3.0",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest-dom": "4.0.2",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.30.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.5.1",
    "eslint-plugin-unicorn": "43.0.2",
    "husky": "8.0.1",
    "i18next-parser": "6.5.0",
    "jsdom": "20.0.0",
    "less": "4.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "source-map-explorer": "2.5.2",
    "typescript": "4.7.4",
    "unplugin-moment-to-dayjs": "0.4.0",
    "vite": "3.0.3",
    "vite-plugin-checker": "0.4.9",
    "vite-plugin-svgr": "2.2.1",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.19.0",
    "vitest-fetch-mock": "0.2.1"
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "antd": "4.21.7",
    "classnames": "2.3.1",
    "dayjs": "1.11.4",
    "history": "5.3.0",
    "i18next": "21.8.14",
    "i18next-http-backend": "1.4.1",
    "oaf-react-router": "3.0.0",
    "query-string": "7.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-i18next": "11.18.1",
    "react-router-dom": "6.3.0",
    "recoil": "0.7.4",
    "recoil-nexus": "0.4.0",
    "recoil-persist": "4.2.0",
    "swr": "1.3.0",
    "use-debounce": "8.0.2"
  }
}
