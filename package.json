{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React TypeScript application starter",
  "private": true,
  "author": "Martin Litvaj <kamahl19@gmail.com> (http://litvaj.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kamahl19/react-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Kamahl19/react-starter/issues"
  },
  "engines": {
    "node": ">=18.0",
    "npm": ">=8.0",
    "yarn": "YARN NO LONGER USED - use npm instead."
  },
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "build": "vite build",
    "format": "prettier --write . --ignore-unknown",
    "format-check": "prettier --check . --ignore-unknown",
    "extract-translations": "i18next",
    "lint": "eslint \"./**/*.{js,ts,tsx}\" --max-warnings 0",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "msw": {
    "workerDirectory": "."
  },
  "devDependencies": {
    "@mswjs/data": "0.11.0",
    "@tanstack/eslint-plugin-query": "4.20.8",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "@vitejs/plugin-react": "3.0.0",
    "@vitest/ui": "0.26.3",
    "autoprefixer": "10.4.13",
    "confusing-browser-globals": "1.0.11",
    "cross-fetch": "3.1.5",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jest-dom": "4.0.3",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.9.1",
    "eslint-plugin-unicorn": "45.0.2",
    "husky": "8.0.2",
    "i18next-parser": "7.1.0",
    "jsdom": "20.0.3",
    "less": "4.1.3",
    "lint-staged": "13.1.0",
    "mock-match-media": "0.4.2",
    "msw": "0.49.2",
    "prettier": "2.8.1",
    "source-map-explorer": "2.5.3",
    "typescript": "4.9.4",
    "vite": "4.0.3",
    "vite-plugin-checker": "0.5.3",
    "vite-plugin-svgr": "2.4.0",
    "vite-tsconfig-paths": "4.0.3",
    "vitest": "0.26.3"
  },
  "dependencies": {
    "@ant-design/icons": "4.8.0",
    "@sindresorhus/is": "5.3.0",
    "@tanstack/react-query": "4.20.4",
    "@tanstack/react-query-devtools": "4.20.4",
    "antd": "4.24.7",
    "classnames": "2.3.2",
    "i18next": "22.4.6",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-i18next": "12.1.4",
    "react-router-dom": "6.6.1",
    "recoil": "0.7.6",
    "recoil-nexus": "0.4.0",
    "recoil-persist": "4.2.0",
    "use-debounce": "9.0.2",
    "use-react-router-breadcrumbs": "4.0.1"
  }
}
