{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React TypeScript application starter",
  "private": true,
  "author": "Martin Litvaj <kamahl19@gmail.com> (http://litvaj.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kamahl19/react-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Kamahl19/react-starter/issues"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0",
    "npm": ">=8.0"
  },
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "visualize": "export VISUALIZE=true && npm run build",
    "build": "vite build",
    "format": "prettier --write . --ignore-unknown",
    "format-check": "prettier --check . --ignore-unknown",
    "i18n": "i18next",
    "lint": "eslint \"./**/*.{js,cjs,mjs,ts,tsx}\" --max-warnings 0",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.11.0",
    "@julr/vite-plugin-validate-env": "0.2.2",
    "@mswjs/data": "0.13.0",
    "@swc/plugin-emotion": "2.5.68",
    "@tanstack/eslint-plugin-query": "4.29.9",
    "@testing-library/react": "13.4.0",
    "@testing-library/user-event": "14.4.3",
    "@total-typescript/ts-reset": "0.4.2",
    "@types/node": "20.3.1",
    "@types/react": "18.2.13",
    "@types/react-dom": "18.2.6",
    "@typescript-eslint/eslint-plugin": "5.60.0",
    "@typescript-eslint/parser": "5.60.0",
    "@vitejs/plugin-react-swc": "3.3.2",
    "@vitest/ui": "0.31.4",
    "confusing-browser-globals": "1.0.11",
    "eslint": "8.43.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jest-dom": "5.0.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.11.0",
    "eslint-plugin-unicorn": "47.0.0",
    "eslint-plugin-vitest": "0.2.6",
    "husky": "8.0.3",
    "i18next-parser": "8.0.0",
    "jsdom": "22.1.0",
    "jsdom-testing-mocks": "1.9.0",
    "lint-staged": "13.2.2",
    "msw": "1.2.2",
    "prettier": "2.8.8",
    "rollup-plugin-visualizer": "5.9.2",
    "source-map-explorer": "2.5.3",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vite-plugin-checker": "0.6.1",
    "vite-plugin-react-click-to-component": "2.0.0",
    "vite-plugin-svgr": "3.2.0",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.31.4",
    "vitest-dom": "0.1.0"
  },
  "dependencies": {
    "@ant-design/icons": "5.1.4",
    "@emotion/react": "11.11.1",
    "@sindresorhus/is": "5.4.1",
    "@tanstack/react-query": "4.29.15",
    "@tanstack/react-query-devtools": "4.29.15",
    "@tshttp/status": "2.0.0",
    "antd": "5.6.2",
    "dayjs": "1.11.8",
    "deepmerge-ts": "5.1.0",
    "i18next": "23.2.3",
    "i18next-browser-languagedetector": "7.0.2",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.10",
    "react-i18next": "13.0.1",
    "react-router-dom": "6.13.0",
    "recoil": "0.7.7",
    "recoil-nexus": "0.5.0",
    "recoil-persist": "4.2.0",
    "use-debounce": "9.0.4",
    "use-query-params": "2.2.1",
    "use-react-router-breadcrumbs": "4.0.1",
    "zod": "3.21.4",
    "zod-fetch": "0.1.1"
  }
}
