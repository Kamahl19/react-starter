{
  "name": "react-starter",
  "version": "2.0.0",
  "description": "Full-featured starter for React application",
  "author": "Martin Litvaj <kamahl19@gmail.com> (http://litvaj.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kamahl19/react-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Kamahl19/react-starter/issues"
  },
  "engines": {
    "node": ">=10.15.3",
    "yarn": ">=1.15.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "yarn less:build && react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write '**/*.{js,json,less,md}'",
    "extract-translations": "i18next-scanner --config i18next-scanner.config.js",
    "less:build": "lessc --js --rewrite-urls=all --include-path=node_modules src/app/styles/main.less src/app/styles/main.css",
    "less:watch": "yarn less:build && nodemon -e less -x \"yarn less:build\"",
    "lint": "node ./node_modules/eslint/bin/eslint.js '**/*.js'",
    "start": "npm-run-all -p less:watch start-js",
    "start-js": "react-scripts start",
    "storybook": "npm-run-all -p storybook:start less:watch",
    "storybook:start": "start-storybook -p 9001 -s public -c storybook",
    "storybook:build": "build-storybook -s public -c storybook",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint --fix",
      "git add"
    ],
    "*.{js,json,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "5.1.8",
    "@storybook/addon-actions": "5.1.8",
    "@storybook/addon-knobs": "5.1.8",
    "@storybook/addon-viewport": "5.1.8",
    "@storybook/addons": "5.1.8",
    "@storybook/react": "5.1.8",
    "babel-core": "6.26.3",
    "husky": "2.4.0",
    "i18next-scanner": "2.10.2",
    "less": "3.9.0",
    "lint-staged": "8.2.0",
    "nodemon": "1.19.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "react-scripts": "3.0.1",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "3.0.6",
    "source-map-explorer": "1.8.0",
    "storybook-react-router": "1.0.5"
  },
  "dependencies": {
    "antd": "3.19.3",
    "axios": "0.19.0",
    "connected-react-router": "6.4.0",
    "deep-freeze": "0.0.1",
    "flat": "4.1.0",
    "history": "4.9.0",
    "i18next": "15.1.3",
    "localforage": "1.7.3",
    "lodash.flattendeep": "4.4.0",
    "lodash.omit": "4.5.0",
    "lodash.pick": "4.4.0",
    "prop-types": "15.7.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-i18next": "10.11.0",
    "react-redux": "7.0.3",
    "react-responsive": "7.0.0",
    "react-router-dom": "5.0.1",
    "redux": "4.0.1",
    "redux-auth-wrapper": "2.1.0",
    "redux-persist": "5.10.0",
    "redux-saga": "1.0.2",
    "reselect": "4.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
