{
  "name": "react-starter",
  "version": "3.0.0",
  "description": "Full-featured typescript starter for React application",
  "author": "Martin Litvaj <kamahl19@gmail.com> (http://litvaj.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kamahl19/react-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Kamahl19/react-starter/issues"
  },
  "engines": {
    "node": ">=10.15.3",
    "yarn": ">=1.15.2"
  },
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "yarn less:build && react-scripts build",
    "eject": "react-scripts eject",
    "format": "prettier --write '**/*.{ts,tsx,json,less,md}'",
    "extract-translations": "i18next-scanner",
    "less:build": "lessc --js --rewrite-urls=all --include-path=node_modules src/app/styles/main.less src/app/styles/main.css",
    "less:watch": "yarn less:build && nodemon -e less -x \"yarn less:build\"",
    "lint": "node ./node_modules/eslint/bin/eslint.js '**/*.{ts,tsx}'",
    "start": "npm-run-all -p less:watch start-cra",
    "start-cra": "react-scripts start",
    "storybook": "npm-run-all -p storybook:start less:watch",
    "storybook:start": "start-storybook -p 9001 -s public -c storybook",
    "storybook:build": "build-storybook -s public -c storybook",
    "test": "react-scripts test"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint --fix",
      "git add"
    ],
    "*.{ts,tsx,json,less,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "5.1.9",
    "@storybook/addon-actions": "5.1.9",
    "@storybook/addon-knobs": "5.1.9",
    "@storybook/addon-viewport": "5.1.9",
    "@storybook/addons": "5.1.9",
    "@storybook/react": "5.1.9",
    "@types/deep-freeze": "^0.1.1",
    "@types/flat": "0.0.28",
    "@types/i18next-xhr-backend": "^1.4.2",
    "@types/jest": "24.0.15",
    "@types/lodash.flattendeep": "^4.4.6",
    "@types/lodash.pick": "4.4.6",
    "@types/node": "12.6.2",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-redux": "7.1.1",
    "@types/react-responsive": "3.0.3",
    "@types/react-router-dom": "4.3.4",
    "@types/redux-auth-wrapper": "^2.0.9",
    "@types/storybook-react-router": "^1.0.0",
    "@types/storybook__addon-a11y": "^5.0.0",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-knobs": "^5.0.2",
    "@types/storybook__addon-viewport": "^4.1.1",
    "@types/storybook__react": "4.0.2",
    "babel-core": "6.26.3",
    "husky": "3.0.0",
    "i18next-scanner": "2.10.2",
    "i18next-scanner-typescript": "^1.0.3",
    "less": "3.9.0",
    "lint-staged": "9.2.0",
    "nodemon": "1.19.1",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "react-scripts": "3.0.1",
    "redux-immutable-state-invariant": "2.1.0",
    "redux-logger": "3.0.6",
    "source-map-explorer": "2.0.1",
    "storybook-react-router": "1.0.5",
    "typescript": "3.5.3"
  },
  "dependencies": {
    "antd": "3.20.1",
    "axios": "0.18.1",
    "connected-react-router": "6.5.0",
    "deep-freeze": "0.0.1",
    "flat": "4.1.0",
    "history": "4.9.0",
    "i18next": "17.0.6",
    "i18next-xhr-backend": "^3.0.0",
    "localforage": "1.7.3",
    "lodash.pick": "4.4.0",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-i18next": "10.11.4",
    "react-redux": "7.1.0",
    "react-responsive": "7.0.0",
    "react-router-dom": "5.0.1",
    "redux": "4.0.4",
    "redux-auth-wrapper": "2.1.0",
    "redux-devtools-extension": "2.13.8",
    "redux-persist": "5.10.0",
    "redux-saga": "1.0.5",
    "reselect": "4.0.0",
    "use-react-router": "^1.0.7"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
