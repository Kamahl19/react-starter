{
  "name": "react-starter",
  "version": "1.0.0",
  "private": true,
  "description": "React TypeScript application starter",
  "bugs": {
    "url": "https://github.com/Kamahl19/react-starter/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kamahl19/react-starter.git"
  },
  "license": "MIT",
  "author": "Martin Litvaj <kamahl19@gmail.com> (http://litvaj.com)",
  "type": "module",
  "scripts": {
    "analyze": "npm run build && source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "build": "tsc --noEmit && vite build",
    "format": "prettier --write . --ignore-unknown",
    "format-check": "prettier --check . --ignore-unknown",
    "i18n": "i18next",
    "lint": "eslint \"./**/*.{js,cjs,mjs,ts,tsx}\" --max-warnings 0",
    "prepare": "husky install",
    "preview": "export IS_PREVIEW=true && vite build && vite preview --port 3000",
    "start": "vite",
    "test": "DEBUG_PRINT_LIMIT=100000 vitest",
    "test:ui": "DEBUG_PRINT_LIMIT=100000 vitest --ui",
    "visualize": "export VISUALIZE=true && npm run build"
  },
  "lint-staged": {
    "*.{js,cjs,mjs,ts,tsx}": [
      "eslint --fix"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "5.2.6",
    "@emotion/react": "11.11.1",
    "@reflet/http": "1.0.0",
    "@tanstack/react-query": "5.8.9",
    "@tanstack/react-query-devtools": "5.8.9",
    "antd": "5.11.5",
    "dayjs": "1.11.10",
    "deepmerge-ts": "5.1.0",
    "i18next": "23.7.7",
    "i18next-browser-languagedetector": "7.2.0",
    "jotai": "2.6.0",
    "jotai-devtools": "0.7.0",
    "query-string": "8.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.11",
    "react-i18next": "13.5.0",
    "react-router-dom": "6.20.0",
    "use-debounce": "10.0.0",
    "use-query-params": "2.2.1",
    "use-react-router-breadcrumbs": "4.0.1",
    "zod": "3.22.4",
    "zod-fetch": "0.1.1"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "11.11.0",
    "@eslint-community/eslint-plugin-eslint-comments": "4.1.0",
    "@julr/vite-plugin-validate-env": "1.0.0",
    "@mswjs/data": "0.16.1",
    "@swc-jotai/debug-label": "0.1.0",
    "@swc-jotai/react-refresh": "0.1.0",
    "@swc/plugin-emotion": "2.5.105",
    "@tanstack/eslint-plugin-query": "5.8.4",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "14.5.1",
    "@total-typescript/ts-reset": "0.5.1",
    "@tsconfig/strictest": "2.0.2",
    "@tsconfig/vite-react": "2.0.1",
    "@types/node": "20.10.0",
    "@types/react": "18.2.39",
    "@types/react-dom": "18.2.17",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-react-swc": "3.5.0",
    "@vitest/ui": "0.34.6",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "npm:eslint-plugin-i@2.29.0",
    "eslint-plugin-jest-dom": "5.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-react-prefer-function-component": "3.3.0",
    "eslint-plugin-sonarjs": "0.23.0",
    "eslint-plugin-testing-library": "6.2.0",
    "eslint-plugin-unicorn": "49.0.0",
    "eslint-plugin-vitest": "0.3.10",
    "husky": "8.0.3",
    "i18next-parser": "8.9.0",
    "jsdom": "23.0.0",
    "jsdom-testing-mocks": "1.11.0",
    "lint-staged": "15.1.0",
    "msw": "2.0.9",
    "prettier": "3.1.0",
    "prettier-plugin-packagejson": "2.4.6",
    "rollup-plugin-visualizer": "5.9.3",
    "source-map-explorer": "2.5.3",
    "terser": "5.24.0",
    "typescript": "5.2.2",
    "vite": "5.0.3",
    "vite-plugin-checker": "0.6.2",
    "vite-plugin-svgr": "4.2.0",
    "vite-tsconfig-paths": "4.2.1",
    "vitest": "0.34.6"
  },
  "engines": {
    "node": "18 || 20",
    "npm": ">=8.0"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
