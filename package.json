{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React TypeScript application starter",
  "private": true,
  "author": "Martin Litvaj <kamahl19@gmail.com> (http://litvaj.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kamahl19/react-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Kamahl19/react-starter/issues"
  },
  "engines": {
    "node": ">=14.18",
    "yarn": "1.x"
  },
  "scripts": {
    "analyze": "yarn build && source-map-explorer 'dist/assets/*.js' --no-border-checks",
    "build": "vite build",
    "format": "prettier --write . --ignore-unknown",
    "format-check": "prettier --check . --ignore-unknown",
    "extract-translations": "i18next",
    "lint": "eslint \"./**/*.{js,ts,tsx}\" --cache",
    "preview": "vite preview",
    "start": "vite",
    "test": "vitest",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix --cache"
    ],
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.4",
    "@testing-library/react": "13.3.0",
    "@testing-library/user-event": "14.2.1",
    "@types/node": "18.0.0",
    "@types/react": "18.0.14",
    "@types/react-dom": "18.0.5",
    "@types/react-router-dom": "5.3.3",
    "@vitejs/plugin-react": "1.3.2",
    "autoprefixer": "10.4.7",
    "eslint": "8.18.0",
    "eslint-config-canonical": "35.0.1",
    "husky": "8.0.1",
    "i18next-parser": "6.5.0",
    "jsdom": "20.0.0",
    "less": "4.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "source-map-explorer": "2.5.2",
    "typescript": "4.7.4",
    "unplugin-moment-to-dayjs": "0.4.0",
    "vite": "2.9.13",
    "vite-plugin-checker": "0.4.6",
    "vite-plugin-svgr": "2.2.0",
    "vite-tsconfig-paths": "3.5.0",
    "vitest": "0.16.0",
    "vitest-fetch-mock": "0.1.0"
  },
  "dependencies": {
    "@ant-design/icons": "4.7.0",
    "antd": "4.21.4",
    "classnames": "2.3.1",
    "history": "5.3.0",
    "i18next": "21.8.11",
    "i18next-http-backend": "1.4.1",
    "oaf-react-router": "3.0.0",
    "query-string": "7.1.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-i18next": "11.17.3",
    "react-router-dom": "6.3.0",
    "recoil": "0.7.4",
    "recoil-nexus": "0.3.18",
    "recoil-persist": "4.2.0",
    "swr": "1.3.0",
    "use-debounce": "8.0.1"
  }
}
