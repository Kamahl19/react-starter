{
  "name": "react-starter-frontend",
  "version": "1.0.0",
  "description": "Simple starter for React, React Native and Express API server",
  "author": "Martin Litvaj <kamahl19@gmail.com> (http://litvaj.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kamahl19/react-starter.git"
  },
  "bugs": {
    "url": "https://github.com/Kamahl19/react-starter/issues"
  },
  "engines": {
    "node": ">=8.9.0",
    "yarn": ">=1.3.0"
  },
  "scripts": {
    "start": "npm-run-all -p watch-less start-js",
    "start-js": "react-scripts start",
    "watch-less": "yarn build-less && nodemon -e less -x \"yarn build-less\"",
    "build":
      "yarn build-less && react-scripts build && copyfiles -u 1 \"build/**/*\" ../backend/public",
    "build-less":
      "lessc --js --relative-urls --include-path=node_modules src/app/styles/main.less src/app/styles/main.css",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prettify": "prettier --write \"{src,__{tests,mocks}__}/**/*.{js,less}\"",
    "extract-translations":
      "i18next ./src/ --output ./src/app/i18n/locales/ --fileFilter \"*.js\" --locales en --recursive --ignore-variables --key-separator false --namespace-separator false --write-old false",
    "analyze": "source-map-explorer build/static/js/main.*"
  },
  "devDependencies": {
    "copyfiles": "^2.0.0",
    "i18next-parser": "^0.13.0",
    "less": "^3.0.2",
    "nodemon": "^1.17.3",
    "npm-run-all": "^4.1.2",
    "prettier": "^1.12.1",
    "react-scripts": "^1.1.4",
    "redux-logger": "^3.0.6",
    "source-map-explorer": "^1.5.0"
  },
  "dependencies": {
    "antd": "^3.4.3",
    "axios": "^0.18.0",
    "classnames": "^2.2.5",
    "history": "^4.7.2",
    "i18next": "^11.2.3",
    "localforage": "^1.7.1",
    "lodash.flattendeep": "^4.4.0",
    "lodash.reduce": "^4.6.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-i18next": "^7.6.0",
    "react-redux": "^5.0.7",
    "react-responsive": "^4.1.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^4.0.0",
    "redux-auth-wrapper": "^2.0.2",
    "redux-persist": "^5.9.1",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1"
  }
}
